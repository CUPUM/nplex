{
	"plugins": ["prettier-plugin-svelte", "prettier-plugin-jsdoc"],
	"pluginSearchDirs": ["."],
	"useTabs": true,
	"singleQuote": true,
	"trailingComma": "none",
	"printWidth": 100,
	"jsdocDescriptionWithDot": true,
	"jsdocVerticalAlignment": true,
	"jsdocSingleLineComment": false,
	"overrides": [{ "files": "*.svelte", "options": { "parser": "svelte" } }]
}
