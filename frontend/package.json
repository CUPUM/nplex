{
	"name": "frontend",
	"version": "0.0.1",
	"description": "Code frontend pour la plateforme nplex dédiée à la documentation des petits projets exemplaires en aménagement",
	"author": {
		"name": "Emmanuel Beaudry Marchand",
		"email": "emmanuel.beaudry.marchand@umontreal.ca"
	},
	"contributors": [
		"Chaire UNESCO en paysage urbain de l'Université de Montréal",
		"Ville de Montréal",
		"Bureau du Design de Montréal",
		"Université de Montréal"
	],
	"browserslist": [
		"defaults"
	],
	"config": {
		"db-types-output": "src/lib/types/database.ts",
		"dbsource": "local"
	},
	"repository": "github:CUPUM/nplex.git",
	"type": "module",
	"license": "MIT",
	"packageManager": "pnpm@6.25.0",
	"scripts": {
		"gen-db-types": "cd ../backend && supabase gen types typescript --local > ../frontend/src/lib/types/database.ts",
		"gen-remote-db-types": ". ../.env && cd ../backend && supabase gen types typescript --db-url ${SUPABASE_DB_URL} > ../frontend/src/lib/types/database.ts",
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
	},
	"devDependencies": {
		"@sveltejs/adapter-node": "1.0.0-next.104",
		"@sveltejs/kit": "1.0.0-next.581",
		"@types/js-cookie": "^3.0.2",
		"@types/mapbox__mapbox-gl-draw": "^1.3.3",
		"@types/node": "^18.11.13",
		"@types/prettier": "^2.7.1",
		"@typescript-eslint/eslint-plugin": "^5.46.0",
		"@typescript-eslint/parser": "^5.46.0",
		"autoprefixer": "^10.4.13",
		"browserslist": "^4.21.4",
		"csstype": "^3.1.1",
		"element-to-path": "^1.2.1",
		"esbuild": "^0.16.4",
		"eslint": "^8.29.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"mdsvex": "^0.10.6",
		"postcss": "^8.4.19",
		"postcss-load-config": "^4.0.1",
		"postcss-normalize": "^10.0.1",
		"prettier": "^2.8.1",
		"prettier-plugin-jsdoc": "0.4.2",
		"prettier-plugin-svelte": "^2.9.0",
		"sass": "^1.56.2",
		"svelte": "^3.54.0",
		"svelte-check": "^2.10.2",
		"svelte-preprocess": "^5.0.0",
		"svelte-preprocess-cssmodules": "^2.2.3",
		"svgson": "^5.2.1",
		"ts-essentials": "^9.3.0",
		"tslib": "^2.4.1",
		"typescript": "^4.9.4",
		"vite": "^4.0.0"
	},
	"dependencies": {
		"@mapbox/mapbox-gl-draw": "^1.3.0",
		"@supabase/supabase-js": "2.1.3",
		"@turf/area": "^6.5.0",
		"@turf/bbox": "^6.5.0",
		"@turf/circle": "^6.5.0",
		"@turf/distance": "^6.5.0",
		"@turf/helpers": "^6.5.0",
		"@turf/random": "^6.5.0",
		"@types/sharp": "^0.31.0",
		"change-case": "^4.1.2",
		"colord": "^2.9.3",
		"cookie": "^0.5.0",
		"fuse.js": "^6.6.2",
		"js-cookie": "^3.0.1",
		"mapbox-gl-draw-geodesic": "^2.1.3",
		"maplibre-gl": "^2.4.0",
		"sharp": "^0.31.2",
		"zod": "^3.19.1",
		"zod-form-data": "^1.2.4"
	}
}
