{
	"name": "frontend",
	"version": "0.0.1",
	"description": "Code frontend pour la plateforme nplex dédiée à la documentation des petits projets exemplaires en aménagement",
	"author": {
		"name": "Emmanuel Beaudry Marchand",
		"email": "emmanuel.beaudry.marchand@umontreal.ca"
	},
	"contributors": [
		"Chaire UNESCO en paysage urbain de l'Université de Montréal",
		"Ville de Montréal",
		"Bureau du Design de Montréal",
		"Université de Montréal"
	],
	"browserslist": [
		"defaults"
	],
	"config": {
		"db-types-output": "src/lib/types/database/generated.ts"
	},
	"repository": "github:CUPUM/nplex.git",
	"type": "module",
	"license": "MIT",
	"packageManager": "pnpm@7.26.0",
	"scripts": {
		"serve": "node ./build/index.js",
		"db-types-remote": "dotenv -e ../.env -- bash -c 'npx supabase gen types typescript --project-id $SUPABASE_PROJECT_ID --schema public > $npm_package_config_db_types_output --debug'",
		"db-types-local": "cd ../backend && supabase gen types typescript --local > ../frontend/$npm_package_config_db_types_output",
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
	},
	"devDependencies": {
		"@sveltejs/adapter-vercel": "^2.4.2",
		"@sveltejs/kit": "1.15.7",
		"@types/dompurify": "^3.0.1",
		"@types/js-cookie": "^3.0.3",
		"@types/mapbox__mapbox-gl-draw": "^1.3.3",
		"@types/marked": "^4.0.8",
		"@types/node": "^18.15.11",
		"@types/prettier": "^2.7.2",
		"@typescript-eslint/eslint-plugin": "^5.59.0",
		"@typescript-eslint/parser": "^5.59.0",
		"autoprefixer": "^10.4.14",
		"browserslist": "^4.21.5",
		"csstype": "^3.1.2",
		"element-to-path": "^1.2.1",
		"esbuild": "^0.17.17",
		"eslint": "^8.38.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"mdsvex": "^0.10.6",
		"postcss": "^8.4.22",
		"postcss-load-config": "^4.0.1",
		"postcss-normalize": "^10.0.1",
		"prettier": "^2.8.7",
		"prettier-plugin-jsdoc": "0.4.2",
		"prettier-plugin-svelte": "^2.10.0",
		"sass": "^1.62.0",
		"supabase": "^1.50.8",
		"svelte": "^3.58.0",
		"svelte-check": "^3.2.0",
		"svelte-preprocess": "^5.0.3",
		"svg-path-properties": "^1.1.0",
		"svgson": "^5.2.1",
		"ts-essentials": "^9.3.1",
		"tslib": "^2.5.0",
		"typescript": "^5.0.4",
		"vite": "^4.2.2"
	},
	"dependencies": {
		"@mapbox/mapbox-gl-draw": "1.4.1",
		"@supabase/supabase-js": "2.20.0",
		"@turf/turf": "^6.5.0",
		"@types/sharp": "^0.31.1",
		"@types/sortablejs": "^1.15.1",
		"change-case": "^4.1.2",
		"colord": "^2.9.3",
		"dompurify": "^3.0.2",
		"fuse.js": "^6.6.2",
		"js-cookie": "^3.0.1",
		"mapbox-gl-draw-geodesic": "^2.1.3",
		"maplibre-gl": "2.4.0",
		"maplibre-gl-draw-circle": "^0.1.1",
		"marked": "^4.3.0",
		"overlayscrollbars": "^2.1.1",
		"panzoom": "^9.4.3",
		"sharp": "^0.32.0",
		"sortablejs": "^1.15.0",
		"zod": "^3.21.4",
		"zod-form-data": "^2.0.1"
	}
}
