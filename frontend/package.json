{
	"name": "nplex-frontend",
	"version": "0.0.1",
	"description": "Code frontend pour la plateforme nplex dédiée à la documentation des petits projets exemplaires en aménagement",
	"author": {
		"name": "Emmanuel Beaudry Marchand",
		"email": "emmanuel.beaudry.marchand@umontreal.ca"
	},
	"contributors": [
		"Chaire UNESCO en paysage urbain de l'Université de Montréal",
		"Ville de Montréal",
		"Bureau du Design de Montréal",
		"Université de Montréal"
	],
	"browserslist": [
		"defaults"
	],
	"repository": "github:CUPUM/nplex.git",
	"type": "module",
	"license": "MIT",
	"packageManager": "pnpm@6.25.0",
	"scripts": {
		"icons": "esmo ./scripts/ICONS.ts",
		"cssvars": "esmo ./scripts/CSS_VARS.ts",
		"get-db-types": ". ./.env && npx openapi-typescript \"${PUBLIC_SUPABASE_URL}/rest/v1/?apikey=${PUBLIC_SUPABASE_ANON_KEY}\" --version=2 --output src/types/database.ts",
		"dev": "svelte-kit dev",
		"build": "pnpm svelte-kit build",
		"preview": "svelte-kit preview",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
	},
	"devDependencies": {
		"@sveltejs/adapter-node": "next",
		"@sveltejs/kit": "next",
		"@types/node": "^17.0.33",
		"@types/overlayscrollbars": "^1.12.1",
		"@typescript-eslint/eslint-plugin": "^5.23.0",
		"@typescript-eslint/parser": "^5.23.0",
		"autoprefixer": "^10.4.7",
		"browserslist": "^4.20.3",
		"color-convert": "^2.0.1",
		"cssnano": "^5.1.7",
		"element-to-path": "^1.2.1",
		"eslint": "^8.15.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^3.4.1",
		"esmo": "^0.14.1",
		"fast-glob": "^3.2.11",
		"postcss": "^8.4.13",
		"postcss-font-magician": "^3.0.0",
		"postcss-load-config": "^3.1.4",
		"postcss-nesting": "^10.1.5",
		"postcss-normalize": "^10.0.1",
		"prettier": "^2.6.2",
		"prettier-plugin-jsdoc": "^0.3.38",
		"prettier-plugin-svelte": "^2.7.0",
		"svelte": "^3.48.0",
		"svelte-check": "^2.7.1",
		"svelte-preprocess": "^4.10.6",
		"svgson": "^5.2.1",
		"tslib": "^2.4.0",
		"typescript": "^4.6.4"
	},
	"dependencies": {
		"@supabase/supabase-js": "^1.35.3",
		"@types/cookie": "^0.4.1",
		"@types/jsonwebtoken": "^8.5.8",
		"cookie": "^0.4.2",
		"jsonwebtoken": "^8.5.1",
		"maplibre-gl": "^2.1.9",
		"overlayscrollbars": "^1.13.1"
	}
}
