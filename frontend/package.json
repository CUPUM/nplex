{
	"name": "frontend",
	"version": "0.0.1",
	"description": "Code frontend pour la plateforme nplex dédiée à la documentation des petits projets exemplaires en aménagement",
	"author": {
		"name": "Emmanuel Beaudry Marchand",
		"email": "emmanuel.beaudry.marchand@umontreal.ca"
	},
	"contributors": [
		"Chaire UNESCO en paysage urbain de l'Université de Montréal",
		"Ville de Montréal",
		"Bureau du Design de Montréal",
		"Université de Montréal"
	],
	"browserslist": [
		"defaults"
	],
	"config": {
		"db-types-output": "src/lib/types/database.ts",
		"dbsource": "local"
	},
	"repository": "github:CUPUM/nplex.git",
	"type": "module",
	"license": "MIT",
	"packageManager": "pnpm@7.26.0",
	"scripts": {
		"serve": "node ./build/index.js",
		"gen-db-types": "cd ../backend && supabase gen types typescript --local > ../frontend/src/lib/types/database/generated.ts",
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
	},
	"devDependencies": {
		"@graphql-codegen/cli": "^3.0.0",
		"@sveltejs/adapter-vercel": "^2.0.1",
		"@sveltejs/kit": "1.5.0",
		"@types/js-cookie": "^3.0.2",
		"@types/mapbox__mapbox-gl-draw": "^1.3.3",
		"@types/node": "^18.13.0",
		"@types/prettier": "^2.7.2",
		"@typescript-eslint/eslint-plugin": "^5.51.0",
		"@typescript-eslint/parser": "^5.51.0",
		"autoprefixer": "^10.4.13",
		"browserslist": "^4.21.5",
		"csstype": "^3.1.1",
		"element-to-path": "^1.2.1",
		"esbuild": "^0.17.6",
		"eslint": "^8.33.0",
		"eslint-config-prettier": "^8.6.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"houdini": "^0.20.4",
		"houdini-svelte": "^0.20.4",
		"mdsvex": "^0.10.6",
		"postcss": "^8.4.21",
		"postcss-load-config": "^4.0.1",
		"postcss-normalize": "^10.0.1",
		"prettier": "^2.8.3",
		"prettier-plugin-jsdoc": "0.4.2",
		"prettier-plugin-svelte": "^2.9.0",
		"sass": "^1.58.0",
		"svelte": "^3.55.1",
		"svelte-check": "^3.0.3",
		"svelte-preprocess": "^5.0.1",
		"svelte-preprocess-cssmodules": "^2.2.4",
		"svg-path-properties": "^1.1.0",
		"svgson": "^5.2.1",
		"ts-essentials": "^9.3.0",
		"tslib": "^2.5.0",
		"typescript": "^4.9.5",
		"vite": "^4.1.1"
	},
	"dependencies": {
		"@mapbox/mapbox-gl-draw": "1.4.0",
		"@supabase/supabase-js": "2.7.1",
		"@turf/turf": "^6.5.0",
		"@types/sharp": "^0.31.1",
		"change-case": "^4.1.2",
		"colord": "^2.9.3",
		"cookie": "^0.5.0",
		"fuse.js": "^6.6.2",
		"graphql": "^16.6.0",
		"js-cookie": "^3.0.1",
		"mapbox-gl-draw-geodesic": "^2.1.3",
		"maplibre-gl": "^2.4.0",
		"maplibre-gl-draw-circle": "^0.1.1",
		"sharp": "^0.31.3",
		"zod": "^3.20.2",
		"zod-form-data": "^1.2.4"
	}
}
