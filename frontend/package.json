{
	"name": "frontend",
	"version": "0.0.1",
	"description": "Code frontend pour la plateforme nplex dédiée à la documentation des petits projets exemplaires en aménagement",
	"author": {
		"name": "Emmanuel Beaudry Marchand",
		"email": "emmanuel.beaudry.marchand@umontreal.ca"
	},
	"contributors": [
		"Chaire UNESCO en paysage urbain de l'Université de Montréal",
		"Ville de Montréal",
		"Bureau du Design de Montréal",
		"Université de Montréal"
	],
	"browserslist": [
		"defaults"
	],
	"repository": "github:CUPUM/nplex.git",
	"type": "module",
	"license": "MIT",
	"packageManager": "pnpm@6.25.0",
	"scripts": {
		"gen-remote-db-types": ". ../.env && cd ../backend && supabase gen types typescript --db-url ${SUPABASE_DB_URL} > ../frontend/src/lib/types/database.ts",
		"gen-local-db-types": "cd ../backend && supabase gen types typescript --local > ../frontend/src/lib/types/database.ts",
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
	},
	"devDependencies": {
		"@sveltejs/adapter-node": "1.0.0-next.100",
		"@sveltejs/kit": "1.0.0-next.532",
		"@types/cookie": "^0.5.1",
		"@types/js-cookie": "^3.0.2",
		"@types/node": "^18.11.9",
		"@types/prettier": "^2.7.1",
		"@types/set-cookie-parser": "^2.4.2",
		"@typescript-eslint/eslint-plugin": "^5.42.0",
		"@typescript-eslint/parser": "^5.42.0",
		"autoprefixer": "^10.4.13",
		"browserslist": "^4.21.4",
		"cssnano": "^5.1.14",
		"element-to-path": "^1.2.1",
		"esbuild": "^0.15.13",
		"eslint": "^8.26.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"mdsvex": "^0.10.6",
		"postcss": "^8.4.18",
		"postcss-font-magician": "^3.0.0",
		"postcss-load-config": "^4.0.1",
		"postcss-nested": "^6.0.0",
		"postcss-nesting": "^10.2.0",
		"postcss-normalize": "^10.0.1",
		"prettier": "^2.7.1",
		"prettier-plugin-jsdoc": "0.4.2",
		"prettier-plugin-svelte": "^2.8.0",
		"sass": "^1.56.0",
		"svelte": "^3.52.0",
		"svelte-check": "^2.9.2",
		"svelte-preprocess": "^4.10.7",
		"svelte-preprocess-cssmodules": "^2.2.3",
		"svgson": "^5.2.1",
		"ts-essentials": "^9.3.0",
		"tslib": "^2.4.1",
		"typescript": "^4.8.4",
		"vite": "^3.2.2"
	},
	"dependencies": {
		"@supabase/supabase-js": "2.0.5",
		"@turf/bbox": "^6.5.0",
		"@turf/circle": "^6.5.0",
		"@turf/helpers": "^6.5.0",
		"@turf/random": "^6.5.0",
		"change-case": "^4.1.2",
		"colord": "^2.9.3",
		"cookie": "^0.5.0",
		"js-cookie": "^3.0.1",
		"maplibre-gl": "^2.4.0",
		"zod": "^3.19.1"
	}
}
