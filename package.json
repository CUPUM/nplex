{
	"name": "nplex-frontend",
	"version": "0.0.1",
	"description": "Code frontend pour la plateforme nplex dédiée à la documentation des petits projets exemplaires en aménagement",
	"author": {
		"name": "Emmanuel Beaudry Marchand",
		"email": "emmanuel.beaudry.marchand@umontreal.ca"
	},
	"contributors": [
		"Chaire UNESCO en paysage urbain de l'Université de Montréal",
		"Ville de Montréal",
		"Université de Montréal"
	],
	"repository": {
		"url": "https://github.com/CUPUM/nplex-frontend",
		"type": "git"
	},
	"type": "module",
	"license": "MIT",
	"packageManager": "pnpm@6.25.0",
	"scripts": {
		"icons": "esmo ./scripts/ICONS.ts",
		"cssvars": "esmo ./scripts/CSS_VARS.ts",
		"dev": "svelte-kit dev",
		"build": "pnpm icons && pnpm cssvars && pnpm svelte-kit build",
		"preview": "svelte-kit preview",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
	},
	"devDependencies": {
		"@sveltejs/adapter-node": "next",
		"@sveltejs/kit": "next",
		"@types/node": "^17.0.10",
		"@typescript-eslint/eslint-plugin": "^5.10.0",
		"@typescript-eslint/parser": "^5.10.0",
		"browserslist": "^4.19.1",
		"element-to-path": "^1.2.1",
		"eslint": "^8.7.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^3.4.0",
		"esmo": "^0.14.0",
		"fast-glob": "^3.2.11",
		"prettier": "^2.5.1",
		"prettier-plugin-jsdoc": "^0.3.30",
		"prettier-plugin-svelte": "^2.6.0",
		"svelte": "^3.46.2",
		"svelte-check": "^2.3.0",
		"svelte-preprocess": "^4.10.2",
		"svgson": "^5.2.1",
		"tslib": "^2.3.1",
		"typescript": "^4.5.5"
	},
	"dependencies": {
		"@supabase/supabase-js": "^1.29.4",
		"@types/overlayscrollbars": "^1.12.1",
		"maplibre-gl": "^1.15.2",
		"overlayscrollbars": "^1.13.1"
	}
}
