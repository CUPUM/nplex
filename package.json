	"engines": {
		"pnpm": "^8.6.0"
	},
	"packageManager": "pnpm@8.6.0",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"db:run": "tsx --tsconfig ./.svelte-kit/tsconfig.json node_modules/drizzle-kit/index.cjs",
		"db:gen": "pnpm db:run generate:pg",
		"db:drop": "pnpm db:run drop",
		"db:studio": "pnpm db:run studio",
		"db:apply": "tsx --tsconfig ./.svelte-kit/tsconfig.json ./scripts/apply.ts",
		"db:seed": "tsx --tsconfig ./.svelte-kit/tsconfig.json ./scripts/seed.ts",
		"db:sketch": "tsx --tsconfig ./.svelte-kit/tsconfig.json ./scripts/sketch.ts",
		"db:scaffold": "pnpm db:apply && pnpm db:seed"
	},
	"devDependencies": {
		"@melt-ui/pp": "^0.1.2",
		"@sveltejs/adapter-auto": "^2.1.0",
		"@sveltejs/adapter-vercel": "^3.0.3",
