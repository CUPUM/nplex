		"autoprefixer": "^10.4.1",
		"browserslist": "^4.19.1",
		"cssnano": "^5.0.14",
		"eslint": "^8.6.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^3.3.0",
		"postcss": "^8.4.5",
		"postcss-each": "^1.1.0",
